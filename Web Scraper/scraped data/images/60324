			var baseurl = "https://collectivehub.com";
		</script>
		<script src="https://collectivehub.com/wp-content/themes/collectivehub/library/js/ajax/ajax.js?v=8"></script>
		<script>

			// Ready
			jQuery(function () {

				// Variables
				var $window = jQuery(window),
					$header = jQuery('body > #container > header'),
					$search = jQuery('body > #container > .search-bar-outer .search-bar-wrap'),
					$advert = jQuery('body > #container > .ad-unit-header'),
					headerFixed = $window.scrollTop() === 0,
					advertFixed = null,
					windowWidth = $window.width(),
					mobileWidth = 481;

				// Check order
				if ($header.prev().is($advert)) {
					
					// Detect scroll
					$window.scroll(function () {
						if ($window.scrollTop() > $advert.height()) {
							if (!headerFixed) {
								headerFixed = true;
								$header.attr('style', 'position:fixed; top:0; z-index:960;');
								$search.attr('style', 'position:fixed; top:80px;');
							}
						} else {
							if (headerFixed) {
								headerFixed = false;
								$header.attr('style', 'position:absolute; z-index:960;');
								$search.attr('style', 'position:absolute; top:' + ($header.offset().top + $header.height()) + 'px;');
							}
						}
					}).trigger('scroll');

					// Detect resize
					$window.resize(function () {
						windowWidth = $window.width();
						if (windowWidth < mobileWidth) {
							if (advertFixed === null || advertFixed === false) {
								advertFixed = true;
								$advert.attr('style', 'position:fixed;');
							}
						} else {
							if (advertFixed === null || advertFixed === true) {
								advertFixed = false;
								$advert.attr('style', 'position:absolute;');
							}
						}
					}).trigger('resize');

				}
				
				// Related articles
				jQuery('.related-articles a').click(function (event) {
					ga('send', 'event', 'Related Article', 'Click', jQuery(this).attr('href'));
				});
				
				// Gallery
				jQuery('.cycle-slideshow').on('cycle-after', function(event, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {
					var $pin = jQuery(this).prev().find('.pin');
					$pin.attr('href', $pin.attr('data-href').replace('{media}', jQuery(incomingSlideEl).find('img').attr('src')));
				});

			});

		</script>

		<!-- Script -->
		<script>

			// Check iframe
			if (window.self !== window.top) {
			
				if (!('remove' in Element.prototype)) {
					Element.prototype.remove = function() {
						if (this.parentNode) {
							this.parentNode.removeChild(this);
						}
					};
				}

				// Change nav
				document.getElementById('MessengerNav').removeAttribute('style');
				document.getElementById('CollectiveNav').remove();

				// Change side nav
				document.getElementById('MessengerSideNav').removeAttribute('style');
				document.getElementById('CollectiveSideNav').remove();

				// Change links
				document.getElementById('HeaderLogo').href = '/lisa-messenger/';
				document.getElementById('HeaderLogoMobile').href = '/lisa-messenger/';
				document.getElementById('sidebar-newsletter-signup').href = '/membership/';
				
				// Change name
				if (document.getElementById('FooterSiteName')) {
					document.getElementById('FooterSiteName').innerHTML = 'Lisa Messenger';
				}

			}

			// Send message
			parent.postMessage({ url: '/2017/05/are-these-stackable-homes-the-answer-to-a-housing-crisis/60324/', title: document.title }, 'https://www.lisamessenger.com');

		</script>

	</body>
</html>
